{
  "permissions": {
    "allow": [
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git branch:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git symbolic-ref:*)",
      "Bash(git rev-parse:*)",
      "Bash(git push origin:*)",
      "Bash(git remote:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh auth status:*)",
      "Bash(gh repo view:*)",
      "Bash(find:*)",
      "Bash(echo:*)",
      "Bash(grep:*)",
      "Bash(wc:*)"
    ],
    "deny": [
      "Bash(git push --force:*)",
      "Bash(git push -f:*)",
      "Bash(git push --force-with-lease:*)",
      "Bash(git push origin --force:*)",
      "Bash(git push origin -f:*)",
      "Bash(git push origin --force-with-lease:*)",
      "Bash(git push --force-with-lease=*:*)",
      "Bash(git push -f origin:*)"
    ]
  }
}
